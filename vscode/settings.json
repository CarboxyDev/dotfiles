{
  "editor.renderWhitespace": "selection",
  "editor.guides.highlightActiveIndentation": true,
  /*
    'Jetbrains Mono','SF Mono','Droid Sans Mono', 'Menlo'
  */
  "editor.fontFamily": "'CommitMono','SF Mono','Droid Sans Mono', 'Menlo'",
  "editor.fontSize": 18,
  "editor.matchBrackets": "always",
  "workbench.startupEditor": "none",
  "workbench.preferredDarkColorTheme": "GitHub Dark Dimmed",
  "workbench.tree.expandMode": "doubleClick",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "window.title": "-",
  "window.titleSeparator": "",
  "update.mode": "manual",
  "emmet.triggerExpansionOnTab": true,
  "html.hover.references": false,
  "editor.scrollbar.vertical": "auto",
  "editor.formatOnPaste": true,
  "workbench.commandPalette.preserveInput": true,
  "workbench.enableExperiments": false,
  "explorer.confirmDelete": false,
  "explorer.autoReveal": true,
  "workbench.tips.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "vsicons.dontShowNewVersionMessage": true,
  "files.associations": {
    "*.pcss": "scss",
    "*.css": "css",
    "*.jsx": "javascriptreact",
    ".clangd": "yaml",
    ".clang-format": "yaml",
    ".skhdrc": "applescript-injection"
  },
  "editor.quickSuggestions": {
    "strings": "inline",
  },
  "editor.guides.indentation": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairs": false,
  "scm.diffDecorations": "none",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.parameterHints.enabled": true,
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.tabSize": 2
  },
  "[prisma]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[cpp]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    //"editor.defaultFormatter": "demiaochen.clang-format-indent-4"
  },
  "[rust]": {
    //"editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "workbench.editorAssociations": {
    "*.dat": "default"
  },
  "prettier.bracketSameLine": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "editor.formatOnSave": true,
  "terminal.integrated.gpuAcceleration": "off",
  "tailwindCSS.experimental.classRegex": [
    [
      "clsx\\(([^)]*)\\)",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ],
    [
      "cva\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ],
    
  ],
  "tailwindCSS.classAttributes": [
    ".*Styles.*",
    "className",
    "class",
    "classNames"
  ],
  "workbench.list.mouseWheelScrollSensitivity": 1.1,
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "icons-carbon",
  "workbench.welcomePage.walkthroughs.openOnInstall": false,
  "breadcrumbs.icons": false,
  "editor.inlineSuggest.enabled": true,
  "debug.onTaskErrors": "showErrors",
  "editor.minimap.enabled": false,
  "editor.wordWrapColumn": 90,
  "editor.smoothScrolling": true,
  "git.autofetch": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#F57156",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "%",
      "color": "#808FC8",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "fixme",
      "color": "#FE7A5F",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "multiple",
  "explorer.decorations.badges": true,
  "workbench.layoutControl.enabled": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "css.lint.unknownAtRules": "ignore",
  "git.openRepositoryInParentFolders": "never",
  "errorLens.enabledDiagnosticLevels": [
    "error",
    "info"
  ], 
  "explorer.compactFolders": false,
  "explorer.decorations.colors": false,
  "git.decorations.enabled": true,
  "C_Cpp.errorSquiggles": "enabled",
  "C_Cpp.intelliSenseEngine": "disabled",
  "editor.inlayHints.enabled": "off",
  "explorer.confirmDragAndDrop": false,
  "clang-format.style": "file",
  "python.venvPath": "/opt/homebrew/opt/python@3.9/bin/python3.9",
  "python.terminal.activateEnvironment": true,
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3.9",
  "editor.accessibilitySupport": "off",
  "editor.lineHeight": 0,
  "window.openFilesInNewWindow": "false",
  "editor.hideCursorInOverviewRuler": true,
  "window.titleBarStyle": "native",
  "apc.electron": {
    "frame": false,
    "titleBarStyle": "customButtonsOnHover",
  },
  "apc.header": {
    "height": 48,
    "fontSize": 16,
  },
  "apc.listRow": {
    "height": 32,
    "fontSize": 16,
  },
  "apc.stylesheet": {
    ".title-label > h2": "display: none", // Remove primary side bar title
    ".title-actions": "display: none", // Remove primary side bar action icons
    ".editor-actions": "display: none", // Remove editor action icons
    ".nosidebar .inline-tabs-placeholder": "width: 75px", // Align tabs to not overlap window controls when primary bar is hidden
    //".monaco-workbench .part>.composite.title": "display: none", 
    ".monaco-breadcrumbs": "font-size: 14px",
    ".monaco-editor .find-widget .monaco-findInput .input": "font-size: 18px !important",
    /** Command palette styles */
    ".quick-input-list .monaco-list .monaco-list-row .monaco-highlighted-label .highlight" : "color: inherit !important; font-weight: inherit !important",
    ".quick-input-list .monaco-list .monaco-list-row.focused .monaco-highlighted-label .highlight": "color: inherit !important; font-weight: inherit !important",
  },
  "workbench.colorTheme": "Qii Dark",
  "search.followSymlinks": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.lightbulb.enabled": "off",
  /** Super heavy since this scans the whole project **/
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true
  },
  "console-ninja.featureSet": "Community",
  "errorLens.messageEnabled": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": false
  },

  /** For custom typescript syntax highlighting */
  "editor.tokenColorCustomizations": {
    // "[Carboxy]": {
    //   "textMateRules": [
    //     {
    //       "scope": [
    //         "entity.name.type.interface.tsx", // Function declarations
    //       ],
    //       "settings": {
    //         "foreground": "#da4caa"
    //       }
    //     }, 
    //   ]
    // }
  },

  "editor.semanticTokenColorCustomizations": {},
  "importCost.fontStyle": "italic",
  "accessibility.signals.lineHasWarning": {
    "sound": "auto"
  },
  "workbench.editor.tabActionLocation": "right",
  "workbench.editor.empty.hint": "hidden",
  "window.commandCenter": false,
  "workbench.iconTheme": "eq-material-theme-icons-palenight",
  "workbench.editor.customLabels.patterns": {
    "**/app/**/page.tsx": "${dirname} - Page.tsx",
    "**/app/**/layout.tsx": "${dirname} - Layout.tsx"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  //"editor.occurrencesHighlight": "off"
  "workbench.colorCustomizations": {
    "editor.wordHighlightBorder": "#2D313E",
  },
  "terminal.integrated.env.osx": {},
  "workbench.quickOpen.closeOnFocusLost": false,
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.exclude": {
    "**/.next": true
  },
  "workbench.sideBar.location": "right",
  "haskell.manageHLS": "GHCup",
  "gitlens.blame.fontFamily": "Wotfard",
  "powermode.enabled": false,
  "gitlens.mode.active": "zen",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "console-ninja.dateTimeFormat": "HH:mm:ss",
  "console-ninja.fontSize": 18,
  "security.promptForLocalFileProtocolHandling": false,
  "quickgrep.grep": "Ripgrep",
  "eslint.ignoreUntitled": true,
  "eslint.onIgnoredFiles": "warn",
  "supermaven.enableFixWithSupermaven": true,
  "workbench.activityBar.location": "hidden",

}